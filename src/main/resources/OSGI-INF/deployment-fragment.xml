<?xml version="1.0" encoding="UTF-8"?>

<fragment version="1">
	<extension target="application#MODULE">
		<module>
			<java>${bundle.fileName}</java>
		</module>
	</extension>
	<require>all</require>
	<install>
		<unzip from="${bundle.fileName}" to="/" prefix="web">
			<include>web/nuxeo.war/**</include>
		</unzip>
	</install>
</fragment>